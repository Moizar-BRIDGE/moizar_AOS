<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile_interested_etc">

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        android:transitionName="imageTransition5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <View
            android:id="@+id/ellipse_68"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/solid_view" />

        <TextView
            android:id="@+id/madetext"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/notosans_bold"
            android:text="자신만의 프로필을 만들어보세요"
            android:textColor="@color/black"
            android:textSize="25sp"/>

        <TextView
            android:id="@+id/step_2"
            android:layout_width="78dp"
            android:layout_height="50dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="110dp"
            android:fontFamily="@font/notosans_medium"
            android:text="Step 3"
            android:textColor="@color/Step1_color"
            android:textSize="23sp" />
    </FrameLayout>



    <com.google.android.material.chip.ChipGroup
        android:id="@+id/chipGroup"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout">


    </com.google.android.material.chip.ChipGroup>
    <FrameLayout
        android:id="@+id/frameLayout4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chipGroup">

        <ImageView
            android:id="@+id/etc_view"
            android:layout_width="335dp"
            android:layout_height="390dp"
            android:transitionName="imageTransition4"
            android:elevation="3dp"
            android:layout_marginStart="40dp"
            android:background="@drawable/step3_chip"
            android:fontFamily="@font/notosans_medium"
            android:gravity="center"
            android:textSize="20sp" />

        <TextView
            android:transitionName="imageTransition9"
            android:id="@+id/etc_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="3dp"
            android:layout_marginStart="60dp"
            android:fontFamily="@font/notosans_medium"
            android:gravity="center"
            android:text="기타"
            android:textColor="#FF0063FB"
            android:textSize="25sp"/>
        <Button
            android:id="@+id/completebutton"
            android:layout_width="120dp"
            android:layout_height="44dp"
            android:layout_marginTop="330dp"
            android:layout_marginStart="220dp"
            android:background="@drawable/solid_button"
            app:backgroundTint="#FF0063FB"
            android:text="완료"
            />
        <Button
            android:id="@+id/direc_btn"
            android:layout_width="120dp"
            android:layout_height="44dp"
            android:layout_marginTop="330dp"
            android:layout_marginStart="70dp"
            android:background="@drawable/solid_button"
            app:backgroundTint="#FFEAEAEA"
            android:textColor="#FF565656"
            android:text="직접 추가하기"
            android:textSize="12sp"
            />
        <ScrollView
            android:id="@+id/scrollview1"
            android:layout_width="270dp"
            android:layout_height="250dp"
            android:layout_marginStart="70dp"
            android:layout_marginTop="50dp"
            android:scrollbars="none"
            android:fillViewport="true"
            app:layout_constraintTop_toBottomOf="@+id/chipGroup"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <com.google.android.material.chip.ChipGroup
                    android:id="@+id/chipGroup1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="20dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/linearLayout">

                    <com.google.android.material.chip.Chip
                        android:id="@+id/etc_contents"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:text="콘텐츠 크리에이터"
                        android:textAppearance="@style/chipTextAppearance"
                        android:textColor="@color/Step1_color"
                        app:chipBackgroundColor="@color/white"
                        app:chipStrokeColor="@color/Step1_color"
                        app:chipStrokeWidth="1dp" />

                    <com.google.android.material.chip.Chip
                        android:id="@+id/etc_editer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginStart="30dp"
                        android:text="에디터"
                        android:textAppearance="@style/chipTextAppearance"
                        android:textColor="@color/Step1_color"
                        app:chipBackgroundColor="@color/white"
                        app:chipStrokeColor="@color/Step1_color"
                        app:chipStrokeWidth="1dp" />

                    <com.google.android.material.chip.Chip
                        android:id="@+id/etc_video_edit"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="영상 편집가"
                        android:textAppearance="@style/chipTextAppearance"
                        android:textColor="@color/Step1_color"
                        app:chipBackgroundColor="@color/white"
                        app:chipStrokeColor="@color/Step1_color"
                        app:chipStrokeWidth="1dp" />

                    <com.google.android.material.chip.Chip
                        android:id="@+id/etc_video_prod"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="40dp"
                        android:text="비디오 제작"
                        android:textAppearance="@style/chipTextAppearance"
                        android:textColor="@color/Step1_color"
                        app:chipBackgroundColor="@color/white"
                        app:chipStrokeColor="@color/Step1_color"
                        app:chipStrokeWidth="1dp" />

                    <com.google.android.material.chip.Chip
                        android:id="@+id/etc_Translation"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="통역/번역"
                        android:textAppearance="@style/chipTextAppearance"
                        android:textColor="@color/Step1_color"
                        app:chipBackgroundColor="@color/white"
                        app:chipStrokeColor="@color/Step1_color"
                        app:chipStrokeWidth="1dp" />

                    <com.google.android.material.chip.Chip
                        android:id="@+id/etc_game_plan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="50dp"
                        android:text="게임 기획자"
                        android:textAppearance="@style/chipTextAppearance"
                        android:textColor="@color/Step1_color"
                        app:chipBackgroundColor="@color/white"
                        app:chipStrokeColor="@color/Step1_color"
                        app:chipStrokeWidth="1dp" />

                    <com.google.android.material.chip.Chip
                        android:id="@+id/etc_game_cli"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="게임 클라이언트"
                        android:textAppearance="@style/chipTextAppearance"
                        android:textColor="@color/Step1_color"
                        app:chipBackgroundColor="@color/white"
                        app:chipStrokeColor="@color/Step1_color"
                        app:chipStrokeWidth="1dp" />

                    <com.google.android.material.chip.Chip
                        android:id="@+id/etc_uni"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="40dp"
                        android:text="유니티"
                        android:textAppearance="@style/chipTextAppearance"
                        android:textColor="@color/Step1_color"
                        app:chipBackgroundColor="@color/white"
                        app:chipStrokeColor="@color/Step1_color"
                        app:chipStrokeWidth="1dp" />

                    <com.google.android.material.chip.Chip
                        android:id="@+id/etc_game_gra"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="게임 그래픽"
                        android:textAppearance="@style/chipTextAppearance"
                        android:textColor="@color/Step1_color"
                        app:chipBackgroundColor="@color/white"
                        app:chipStrokeColor="@color/Step1_color"
                        app:chipStrokeWidth="1dp" />

                    <com.google.android.material.chip.Chip
                        android:id="@+id/etc_game_server"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="49dp"
                        android:text="게임 서버"
                        android:textAppearance="@style/chipTextAppearance"
                        android:textColor="@color/Step1_color"
                        app:chipBackgroundColor="@color/white"
                        app:chipStrokeColor="@color/Step1_color"
                        app:chipStrokeWidth="1dp" />

                    <com.google.android.material.chip.Chip
                        android:id="@+id/etc_game_mob"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="모바일 게임 개발"
                        android:textAppearance="@style/chipTextAppearance"
                        android:textColor="@color/Step1_color"
                        app:chipBackgroundColor="@color/white"
                        app:chipStrokeColor="@color/Step1_color"
                        app:chipStrokeWidth="1dp" />

                    <com.google.android.material.chip.Chip
                        android:id="@+id/etc_pd"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="52dp"
                        android:text="PD"
                        android:textAppearance="@style/chipTextAppearance"
                        android:textColor="@color/Step1_color"
                        app:chipBackgroundColor="@color/white"
                        app:chipStrokeColor="@color/Step1_color"
                        app:chipStrokeWidth="1dp" />

                    <com.google.android.material.chip.Chip
                        android:id="@+id/etc_game_art"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="게임 아티스트"
                        android:textAppearance="@style/chipTextAppearance"
                        android:textColor="@color/Step1_color"
                        app:chipBackgroundColor="@color/white"
                        app:chipStrokeColor="@color/Step1_color"
                        app:chipStrokeWidth="1dp" />
                    <com.google.android.material.chip.Chip
                        android:id="@+id/etc_writer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="작가"
                        android:layout_marginStart="67dp"
                        android:textAppearance="@style/chipTextAppearance"
                        android:textColor="@color/Step1_color"
                        app:chipBackgroundColor="@color/white"
                        app:chipStrokeColor="@color/Step1_color"
                        app:chipStrokeWidth="1dp" />


                </com.google.android.material.chip.ChipGroup>
            </LinearLayout>
        </ScrollView>
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>