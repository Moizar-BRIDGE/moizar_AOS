<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile_interested_position">

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <View
            android:id="@+id/ellipse_68"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/solid_view" />

        <TextView
            android:id="@+id/madetext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/notosans_bold"
            android:includeFontPadding="false"
            android:text="자신만의 프로필을\n만들어보세요"
            android:textColor="@color/black"
            android:textSize="25sp"/>
        <TextView
            android:id="@+id/step_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="110dp"
            android:includeFontPadding="false"
            android:fontFamily="@font/notosans_medium"
            android:text="Step 3"
            android:textColor="@color/Step1_color"
            android:textSize="23sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="170dp"
            android:fontFamily="@font/notosans_medium"
            android:includeFontPadding="false"
            android:gravity="center"
            android:text="내가 관심있는\n포지션은 어디인가요?"
            android:textColor="@color/Step1_color"
            android:textSize="20sp" />
        <ImageButton
            android:id="@+id/design_button"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:elevation="3dp"
            android:transitionName="imageTransition"
            android:background="@drawable/cardbuttonselect"
            android:layout_gravity="center"
            android:clickable="true"
            android:layout_marginBottom="30dp"
            android:layout_marginRight="95dp"
            android:fontFamily="@font/notosans_medium"
            android:textSize="20sp" />
        <TextView
            android:id="@+id/design_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:transitionName="imageTransition6"
            android:elevation="3dp"
            android:includeFontPadding="false"
            android:layout_marginBottom="30dp"
            android:layout_gravity="center"
            android:layout_marginRight="95dp"
            android:fontFamily="@font/notosans_medium"
            android:text="디자인"
            android:textSize="20sp" />
        <ImageButton
            android:id="@+id/developer_button"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:elevation="3dp"
            android:layout_marginBottom="30dp"
            android:transitionName="imageTransition2"
            android:background="@drawable/cardbuttonselect"
            android:layout_gravity="center"
            android:layout_marginStart="95dp"
            android:clickable="true"
            android:fontFamily="@font/notosans_medium"
            android:gravity="center"
            android:textSize="20sp" />
        <TextView
            android:id="@+id/developer_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="3dp"
            android:layout_marginBottom="30dp"
            android:includeFontPadding="false"
            android:transitionName="imageTransition7"
            android:layout_gravity="center"
            android:layout_marginStart="95dp"
            android:fontFamily="@font/notosans_medium"
            android:gravity="center"
            android:text="개발자"
            android:textSize="20sp" />
        <ImageButton
            android:id="@+id/marketing_button"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:elevation="3dp"
            android:transitionName="imageTransition3"
            android:layout_gravity="center"
            android:layout_marginRight="95dp"
            android:layout_marginTop="155dp"
            android:background="@drawable/cardbuttonselect"
            android:clickable="true"
            android:fontFamily="@font/notosans_medium"
            android:gravity="center"
            android:textSize="20sp" />
        <TextView
            android:id="@+id/marketing_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="3dp"
            android:transitionName="imageTransition8"
            android:includeFontPadding="false"
            android:layout_gravity="center"
            android:layout_marginRight="95dp"
            android:layout_marginTop="155dp"
            android:fontFamily="@font/notosans_medium"
            android:gravity="center"
            android:text="기획/마케팅"
            android:textSize="20sp" />
        <ImageButton
            android:id="@+id/etc_button"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:elevation="3dp"
            android:transitionName="imageTransition4"
            android:layout_gravity="center"
            android:layout_marginStart="95dp"
            android:layout_marginTop="155dp"
            android:background="@drawable/cardbuttonselect"
            android:clickable="true"
            android:fontFamily="@font/notosans_medium"
            android:gravity="center"
            android:textSize="20sp" />
        <TextView
            android:id="@+id/etc_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="3dp"
            android:includeFontPadding="false"
            android:transitionName="imageTransition9"
            android:layout_gravity="center"
            android:layout_marginStart="95dp"
            android:layout_marginTop="155dp"
            android:fontFamily="@font/notosans_medium"
            android:gravity="center"
            android:text="기타"
            android:textSize="20sp" />
        <Button
            android:id="@+id/step_3_before"
            android:layout_width="120dp"
            android:layout_height="44dp"
            android:layout_marginStart="55dp"
            android:layout_marginTop="650dp"
            android:background="@drawable/solid_button"
            android:text="이전"
            android:textColor="@color/black"
            app:backgroundTint="#FFEAEAEA" />

        <Button
            android:id="@+id/step_3_next"
            android:layout_width="120dp"
            android:layout_height="44dp"
            android:layout_gravity="right"
            android:layout_marginRight="55dp"
            android:layout_marginTop="650dp"
            android:background="@drawable/solid_button"
            android:text="다음"
            app:backgroundTint="@null" />
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>